inner join DM_NORTHWIND.dbo.DIM_ORDEN_COMPRA ON Orders.OrderID=DM_NORTHWIND.dbo.DIM_ORDEN_COMPRA.COD_ORDEN
inner join Employees ON Orders.EmployeeID=Employees.EmployeeID
inner join EmployeeTerritories ON Employees.EmployeeID=EmployeeTerritories.EmployeeID
inner join Territories ON EmployeeTerritories.TerritoryID=Territories.TerritoryID
inner join DM_NORTHWIND.dbo.DIM_TERRITORIO ON Territories.TerritoryID=DM_NORTHWIND.dbo.DIM_TERRITORIO.CODIGO_TERRITORIO</property>
